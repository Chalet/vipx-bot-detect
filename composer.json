{
    "name": "vipx/bot-detect",
    "type": "symfony-bundle",
    "description": "Bot detector library",
    "keywords": ["Bot"],
    "homepage": "http://lennerd.com/",
    "license": "MIT",
    "authors": [
        {
            "name": "Lennart Hildebrandt",
            "email": "hello@lennerd.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4",
        "symfony/yaml": "^2.3|^3.0",
        "symfony/config": "^2.3|^3.0"
    },
    "require-dev": {
        "satooshi/php-coveralls": "^1.0"
    },
    "autoload": {
        "psr-0": { "Vipx\\BotDetect": "" }
    },
    "target-dir": "Vipx/BotDetect",
    "extra": {
        "branch-alias": {
            "dev-master": "1.2.x-dev"
        }
    },
    "config": {
        "platform": {
            "php": "5.4"
        }
    }
}
